{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static",
      "config": {
        "installCommand": "git lfs install && npm install",
        "buildCommand": "git lfs pull && npm run build"
      }
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ]
}